{% extends "layout.njk" %}

{% block head %}
    <title>Sobre -
        {{data.name}}</title>
{% endblock %}

{% block content %}
    <div id="wrapper">
        <img src="{{data.avatar_url}}" alt="{{data.name}}">
        <h1>{{data.name}}</h1>
        <h2>{{data.role}}</h2>
        <p>{{data.description}}</a>
    </p>
</div>

<div class="links social">
    {% for social in data.links %}
        <a href="{{social.url}}">{{social.name}}</a>
    {% endfor %}
</div>

{% endblock %}